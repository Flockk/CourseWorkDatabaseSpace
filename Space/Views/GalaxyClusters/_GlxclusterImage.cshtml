@model Space.Models.GalaxyClusters

@{
	var imgText = "";
	imgText = Model.GlxclusterName.Trim();
}

@if (Model.GlxclusterImage == null)
{
	<img src="/Img/NoImage.png" width="425" height="425" class="img-fluid float-md-start" />
}
else
{
	<img src="@Model.GlxclusterImage" width="450" height="450" class="img-fluid float-md-start" alt="@imgText" title="@imgText" />
}
