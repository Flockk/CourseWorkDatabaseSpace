@model Space.Models.PlanetarySystems

@{
	var imgText = "";
	imgText = Model.PlanetsystemName.Trim();
}

@if (Model.PlanetsystemImage == null)
{
	<img src="/Img/NoImage.png" width="425" height="425" class="img-fluid float-md-start" />
}
else
{
	<img src="@Model.PlanetsystemImage" width="450" height="450" class="img-fluid float-md-start" alt="@imgText" title="@imgText" />
}