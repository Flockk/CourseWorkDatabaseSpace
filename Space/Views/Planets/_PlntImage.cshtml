@model Space.Models.Planets

@{
	var imgText = "";
	imgText = Model.PlntName.Trim();
}

@if (Model.PlntImage == null)
{
	<img src="/Img/NoImage.png" width="425" height="425" class="img-fluid float-md-start" />
}
else
{
	<img src="@Model.PlntImage" width="450" height="450" class="img-fluid float-md-start" alt="@imgText" title="@imgText" />
}