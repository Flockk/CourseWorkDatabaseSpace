@model Space.Models.GalaxyGroups

@{
	var imgText = "";
	imgText = Model.GlxgroupName.Trim();
}

@if (Model.GlxgroupImage == null)
{
	<img src="/Img/NoImage.png" width="425" height="425" class="img-fluid float-md-start" />
}
else
{
	<img src="@Model.GlxgroupImage" width="450" height="450" class="img-fluid float-md-start" alt="@imgText" title="@imgText" />
}
