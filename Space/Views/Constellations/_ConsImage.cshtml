@model Space.Models.Constellations

@{
	var imgText = "";
	imgText = "Созвездие" + " " + Model.ConsName.Trim();
}

@if (Model.ConsImage == null)
{
	<img src="/Img/NoImage.png" width="425" height="425" class="img-fluid float-md-start" alt="@imgText" title="@imgText" />
}
else
{
    <img src="@Model.ConsImage" width="450" height="450" class="img-fluid float-md-start" alt="@imgText" title="@imgText" />
}